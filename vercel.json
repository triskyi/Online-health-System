{
    "version": 2,
    "builds": [
        {
            "src": "voteSystem/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10"
            }
        },
        {
            "src": "requirements.txt",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "voteSystem/wsgi.py"
        }
    ],
    "functions": {
        "build": {
            "handler": "build_files.build",
            "memory": 1024
        }
    }
}